  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.4",
    "@babel/preset-react": "7.13.13",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "5.0.0",
